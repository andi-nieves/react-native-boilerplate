{
  "name": "andiboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint \"{App/**/*.js,*.js}\"",
    "format": "prettier-eslint --write \"{App/**/*.+(js|json),*.+(js|json)}\"",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "fixcode": "standard --fix",
    "lintdiff": "git diff --diff-filter=d --cached --name-only -z -- '*.js' '*.jsx' '*.jsx' | xargs -0 -I % sh -c 'git show \":%\" | ./node_modules/.bin/eslint --stdin --stdin-filename \"%\";'",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf android/app/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "android:production": "react-native run-android",
    "ios:production": "npm run pod && react-native run-ios",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apisauce": "^1.0.2",
    "lodash": "^4.17.11",
    "react": "16.6.3",
    "react-native": "0.58.6",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-vector-icons": "^6.3.0",
    "react-navigation": "^3.3.2",
    "react-navigation-redux-helpers": "1.1.0",
    "react-redux": "^6.0.1",
    "reactotron-react-native": "^3.2.1",
    "reactotron-redux-saga": "^4.2.0",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.2",
    "reduxsauce": "^1.0.1",
    "seamless-immutable": "^7.1.4",
    "standard": "^12.0.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.1.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "0.14.3",
    "jest": "24.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-test-renderer": "16.6.3",
    "reactotron-redux": "^3.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
